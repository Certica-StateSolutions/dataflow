@model DataFlow.Models.Lookup

@Html.AntiForgeryToken()
@Html.ValidationSummary(true, "", new { @class = "text-danger" })
<div class="form-group">
    <div class="col-sm-2">
        @Html.LabelFor(m => m.GroupSet, new { @class = "control-label" })
    </div>
    <div class="col-sm-10">
        @Html.TextBoxFor(m => m.GroupSet, new { @class = "form-control", type = "text", placeholder = "Group Set" })
        @Html.ValidationMessageFor(m => m.GroupSet)
    </div>
</div>
<div class="form-group">
    <div class="col-sm-2">
        @Html.LabelFor(m => m.Key, new { @class = "control-label" })
    </div>
    <div class="col-sm-10">
        @Html.TextBoxFor(m => m.Key, new { @class = "form-control", type = "text", placeholder = "Key" })
        @Html.ValidationMessageFor(m => m.Key)
    </div>
</div>
<div class="form-group">
    <div class="col-sm-2">
        @Html.LabelFor(m => m.Value, new { @class = "control-label" })
    </div>
    <div class="col-sm-10">
        @Html.TextBoxFor(m => m.Value, new { @class = "form-control", type = "text", placeholder = "Value" })
        @Html.ValidationMessageFor(m => m.Value)
    </div>
</div>
<div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
        @Html.HiddenFor(m => m.Id)
        <button id="btnSubmit" type="submit" class="btn btn-primary">Save Lookup</button>
    </div>
</div>